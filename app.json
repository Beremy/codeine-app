{
  "expo": {
    "name": "hostomytho",
    "slug": "hostomytho",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./src/assets/images/icon.png",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#25292e"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hostomytho.hostomytho"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/icon.png"
      },
      "package": "com.hostomytho.hostomytho"
    },
    "web": {
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      [
        "./src/utils/plugins/withDisableForcedDarkModeAndroid.js",
        {}
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "adad8738-a861-4171-8186-e2133adc05bc"
      }
    },
    "owner": "hostomytho"
  }
}
