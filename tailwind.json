{
	"flex": {
		"style": {
			"display": "flex"
		}
	},
	"self-center": {
		"style": {
			"alignSelf": "center"
		}
	},
	"rounded-xl": {
		"style": {
			"borderTopLeftRadius": 12,
			"borderTopRightRadius": 12,
			"borderBottomRightRadius": 12,
			"borderBottomLeftRadius": 12
		}
	},
	"bg-blue-500": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(59 130 246 / var(--tw-bg-opacity))"
		}
	},
	"p-3": {
		"style": {
			"paddingTop": 12,
			"paddingRight": 12,
			"paddingBottom": 12,
			"paddingLeft": 12
		}
	},
	"text-lg": {
		"style": {
			"fontSize": 18,
			"lineHeight": 28
		}
	},
	"font-bold": {
		"style": {
			"fontWeight": "700"
		}
	},
	"font-semibold": {
		"style": {
			"fontWeight": "600"
		}
	},
	"text-white": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(255 255 255 / var(--tw-text-opacity))"
		}
	}
}